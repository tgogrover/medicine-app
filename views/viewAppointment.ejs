<%- include('header') %>
  <div class="container">
        <div class="row">
            <h1>Appointment Lists</h1>
            <br>
         <table class="table table-striped table-primary">
          <thead>
            <tr>
              <th>Appointment Name</th>
        
              <th>Action</th>
            </tr>
          </thead>
          <tbody>
           <% if(Appointment.length >0){
            Appointment.forEach(function(row){
            %>
            <tr>
              <td><%= row.Name %></td>
             
              <td><a class="btn btn-primary btn-xs" href="/api/accept-appointment/edit/<%= row._id %>"><i class="fa fa-edit"></i></a>
                <a class="btn btn-danger btn-xs" href="/view-passwordCategory/delete/<%= row._id %>"><i class="fa fa-trash"></i></a>
              </td>
            </tr>
            <%  }) } %>
          </tbody>
        </table>
      </div>
    </div>
    <%- include('footer') %>